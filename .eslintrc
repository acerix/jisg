{
  "parser": "@babel/eslint-parser",
  "extends": ["prettier"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "max-len": [2, 42069, 4, {"ignoreUrls": true, "ignoreComments": true}],
    "prefer-const": 1,
    "one-var": [0, { "uninitialized": "always", "initialized": "never" }],
    "one-var-declaration-per-line": 0,
    "import/extensions": 0,
    "import/no-unresolved": 0,
    "linebreak-style": "off"
  }
}
