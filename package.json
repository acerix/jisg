{
  "name": "jisg",
  "version": "0.0.1",
  "license": "MIT",
  "engine": "node >= 6.5.x",
  "main": "dist/index.js",
  "description": "Implementations of select OEIS integer sequences in JavaScript.",
  "author": "Dylan R. Ferris",
  "contributors": [
    {
      "name": "Dylan R. Ferris",
      "email": "dylan@psilly.com",
      "uri": "https://psilly.com"
    }
  ],
  "homepage": "https://github.com/acerix/jisg#readme",
  "repository": "git+https://github.com/acerix/jisg.git",
  "bugs": {
    "url": "https://github.com/acerix/jisg/issues"
  },
  "keywords": [
    "integer",
    "sequence",
    "generator",
    "OEIS",
    "es6"
  ],
  "scripts": {
    "lint": "eslint src test",
    "clean": "rimraf dist/",
    "build": "rimraf dist/ && babel src/ --out-dir dist/",
    "test": "./node_modules/.bin/jest",
    "check": "./node_modules/.bin/npm-check -u",
    "example": "node example.js",
    "prepare": "npm run clean && npm run lint && npm run test && npm run build",
    "pub": "./node_modules/.bin/np || true"
  },
  "devDependencies": {
    "babel-cli": "*",
    "babel-core": "*",
    "babel-eslint": "*",
    "babel-plugin-transform-class-properties": "*",
    "babel-plugin-transform-object-rest-spread": "*",
    "babel-preset-env": "*",
    "eslint": "*",
    "eslint-config-airbnb": "*",
    "eslint-config-prettier": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-react": "*",
    "np": "*",
    "npm-check": "*",
    "rimraf": "*",
    "run-sequence": "*"
  },
  "prettier": {
    "singleQuote": true
  }
}
