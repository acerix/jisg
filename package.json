  "version": "0.0.1",
  "license": "MIT",
  "engine": "node >= 15",
  "main": "dist/index.js",
  "description": "Implementations of select OEIS integer sequences in JavaScript.",
  "author": "Dylan Ferris",
  "contributors": [
    {
      "name": "Dylan Ferris",
      "email": "dylan@psilly.com",
